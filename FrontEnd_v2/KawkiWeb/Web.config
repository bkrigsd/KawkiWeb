<?xml version="1.0"?>
<!--
  Para obtener más información sobre cómo configurar la aplicación ASP.NET, visite
  https://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ProductosPortBinding"/>
        <binding name="ProductosVariantesPortBinding"/>
        <binding name="ColoresPortBinding"/>
        <binding name="UsuariosPortBinding"/>
		<binding name="VentasPortBinding" />  
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8080/KawkiWebWS/ProductosService" binding="basicHttpBinding" bindingConfiguration="ProductosPortBinding" contract="KawkiWebWSProductos.Productos" name="ProductosPort"/>
      <endpoint address="http://localhost:8080/KawkiWebWS/ProductosVariantesService" binding="basicHttpBinding" bindingConfiguration="ProductosVariantesPortBinding" contract="KawkiWebWSProductosVariantes.ProductosVariantes" name="ProductosVariantesPort"/>
      <endpoint address="http://localhost:8080/KawkiWebWS/ColoresService" binding="basicHttpBinding" bindingConfiguration="ColoresPortBinding" contract="KawkiWebWSColores.Colores" name="ColoresPort"/>
      <endpoint address="http://localhost:8080/KawkiWebWS/UsuariosService" binding="basicHttpBinding" bindingConfiguration="UsuariosPortBinding" contract="KawkiWebWSUsuarios.Usuarios" name="UsuariosPort"/>
	  <endpoint address="http://localhost:8080/KawkiWebWS/VentasService" binding="basicHttpBinding"  bindingConfiguration="VentasPortBinding" contract="KawkiWebWSVentas.Ventas" name="VentasPort"/> 
	</client>
  </system.serviceModel>
  <system.web>
    <customErrors mode="On" defaultRedirect="Error404.aspx">
      <error statusCode="404" redirect="Error404.aspx"/>
    </customErrors>
    <compilation debug="true"/>
  </system.web>
  <system.webServer>
    <caching>
      <profiles>
        <add extension=".aspx" policy="DisableCache" kernelCachePolicy="DisableCache"/>
      </profiles>
    </caching>
  </system.webServer>
</configuration>